{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ],
    "baseUrl": ".",
    "paths": {
      "@/animation/*": ["animation/*"],
      "@/context/*": ["app/(context)/*"],
      "@/hooks": ["hooks/index.js"],
      "@/ui-components/*": ["ui-components/*"],
      "@/utils/*": ["utils/*"]
    },
    "strictNullChecks": true
  },
  "include": ["next-env.d.ts", ".next/types/**/*.ts", "**/*.ts", "**/*.tsx", "app/offerings/training/events/[slug]/page.jsx", "app/offerings/training/categories/[slug]/page.jsx"],
  "exclude": ["node_modules"]
}
